for notification = 1, 1 do

game:GetService("StarterGui"):SetCore("SendNotification", { 

	Title = "Notification";

	Text = "Thx because you use this script";

	Icon = "rbxthumb://type=Asset&id=11862586975&w=150&h=150"})

Duration = 16;

wait(0.2)

